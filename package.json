{
  "name": "uterine-fibroids-analyzer",
  "version": "1.0.0",
  "description": "AI-powered web application for uterine fibroids detection using U-Net++ with explainable AI",
  "scripts": {
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "cd frontend && npm run dev",
    "start:backend": "cd backend && python main.py",
    "dev": "npm start",
    "build": "cd frontend && npm run build",
    "install:all": "npm run install:frontend && npm run install:backend",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "setup": "./start_webapp.sh",
    "setup:windows": "start_webapp.bat"
  },
  "keywords": [
    "medical-imaging",
    "ai",
    "unet",
    "fibroids",
    "explainable-ai",
    "react",
    "typescript",
    "fastapi",
    "pytorch"
  ],
  "author": "Uterine Fibroids Analyzer Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/uterine-fibroids-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/uterine-fibroids-analyzer/issues"
  },
  "homepage": "https://github.com/your-username/uterine-fibroids-analyzer#readme"
}